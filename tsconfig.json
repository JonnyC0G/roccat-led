{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "allowJs": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "target": "es2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "src/**/*",
    "testing.ts"
  ],
  "exclude": [
    "node_modules",
    "src/khan",
    "**/*.spec.ts"
  ]
}